{
  "name": "randock/model",
  "description": "Randock Model",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Randock\\Model\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Randock\\Model\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "7.1|^8.0"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "doctrine/common": "^2.8|^3.0",
    "randock/event": "dev-develop",
    "symfony/form": "^3.2|^4.0|^5.0|^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "randock/value-object": "dev-develop"
  }
}
